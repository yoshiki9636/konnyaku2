
# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/pi/google-cloud/google-cloud-sdk/path.bash.inc' ]; then source '/home/pi/google-cloud/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/pi/google-cloud/google-cloud-sdk/completion.bash.inc' ]; then source '/home/pi/google-cloud/google-cloud-sdk/completion.bash.inc'; fi

export GOOGLE_APPLICATION_CREDENTIALS='/home/pi/XXXX.json'
